{
  "name": "@tsagent/cli",
  "version": "1.3.1",
  "description": "TsAgent CLI",
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "tsagent-cli": "./dist/main.js"
  },
  "scripts": {
    "start": "tsx src/main.ts --",
    "start:cli": "tsx src/main.ts",
    "build": "cd ../../packages/agent-api && npm run build && cd ../../apps/cli && rm -rf dist && tsc -p tsconfig.json",
    "dev": "tsx src/main.ts",
    "watch": "tsc -w -p tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "@tsagent/core": "^1.3.1",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "ora": "^5.4.1",
    "read": "^4.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "ai",
    "cli",
    "teamspark",
    "workbench"
  ],
  "author": {
    "name": "TeamSpark",
    "email": "support@teamspark.ai"
  },
  "homepage": "https://teamspark.ai",
