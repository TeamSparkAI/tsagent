{
  "name": "@tsagent/semantic-index",
  "version": "1.2.8",
  "description": "CLI tool for semantic indexing and search of agent references",
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "tsagent-semantic-index": "./dist/main.js"
  },
  "scripts": {
    "start": "tsx src/main.ts",
    "build": "cd ../../packages/agent-api && npm run build && cd ../../apps/semantic-index && rm -rf dist && tsc -p tsconfig.json",
    "dev": "tsx src/main.ts",
    "watch": "tsc -w -p tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "@tsagent/core": "^1.2.8",
    "@xenova/transformers": "^2.17.2",
    "chalk": "^4.1.2",
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "ai",
    "cli",
    "semantic-search",
    "embeddings",
    "vector-search"
  ],
  "author": {
    "name": "TeamSpark",
    "email": "support@teamspark.ai"
  },
  "homepage": "https://teamspark.ai",
  "license": "MIT"
}

