{
  "name": "tsagent",
  "version": "1.3.4",
  "private": true,
  "license": "MIT",
  "scripts": {
    "help": "node -e \"console.log(require('fs').readFileSync('scripts/help.txt', 'utf8'))\"",
    "build": "npm run build:desktop && npm run build:cli",
    "build:desktop": "cd apps/desktop && npm run build",
    "build:cli": "cd apps/cli && npm run build",
    "start:desktop": "cd apps/desktop && npm run start",
    "start:cli": "cd apps/cli && npm run start --",
    "dev:desktop": "cd apps/desktop && npm run watch",
    "dev:cli": "cd apps/cli && npm run dev",
    "publish:all": "node scripts/monorepo-manager.mjs publish-all",
    "publish:dry-run": "node scripts/monorepo-manager.mjs publish-all --dry-run",
    "deps:dev": "node scripts/monorepo-manager.mjs dev",
    "deps:publish": "node scripts/monorepo-manager.mjs publish",
    "deps:to-file": "node scripts/monorepo-manager.mjs to-file",
    "deps:to-npm": "node scripts/monorepo-manager.mjs to-npm",
    "deps:install": "node scripts/monorepo-manager.mjs install",
    "deps:clean": "node scripts/monorepo-manager.mjs clean",
    "version:bump": "node scripts/monorepo-manager.mjs bump-version"
  }
}
