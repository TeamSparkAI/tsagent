{
  "name": "@tsagent/server",
  "version": "1.2.7",
  "description": "Simple A2A server that wraps @tsagent/core agents with filesystem strategy",
  "type": "module",
  "bin": {
    "tsagent-server": "dist/cli.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsx src/cli.ts",
    "test": "jest",
    "start": "npx tsx src/cli.ts"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.4",
    "express": "^4.18.2",
    "uuid": "^11.1.0",
    "@tsagent/core": "^1.2.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/uuid": "^10.0.0",
    "@types/jest": "^29.5.14",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0"
  }
}
