{
  "name": "@tsagent/core",
  "version": "1.3.5",
  "description": "Typescript API for agents",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./runtime": {
      "import": "./dist/esm/runtime-esm.js",
      "require": "./dist/cjs/runtime-cjs.js"
    }
  },
  "files": [
    "dist",
    "assets"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "./dist/esm/*"
      ]
    }
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "mkdir -p dist/esm && echo '{\"type\": \"module\"}' > dist/esm/package.json && npx tsc -p tsconfig.esm.json",
    "build:cjs": "mkdir -p dist/cjs && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json && npx tsc -p tsconfig.cjs.json",
    "dev": "npx ts-node src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@aws-sdk/client-bedrock": "^3.787.0",
    "@aws-sdk/client-bedrock-runtime": "^3.785.0",
    "@google/genai": "^1.14.0",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@teamsparkai/1password": "^1.0.1",
    "@types/uuid": "^10.0.0",
    "@xenova/transformers": "^2.17.2",
    "dotenv": "^16.4.7",
    "node-llama-cpp": "^3.14.2",
    "ollama": "^0.5.14",
    "openai": "^6.9.1",
    "trash": "^10.0.0",
    "uuid": "^11.1.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
